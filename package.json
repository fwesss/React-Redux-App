{
  "name": "hmda-explorer",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/fwesss/React-Redux-App.git",
  "author": "Westley Feller <feller.westley@gmail.com>",
  "typings": "./src/react-app-env.d.ts",
  "scripts": {
    "build": "react-scripts build",
    "check-format": "yarn run prettier --list-different",
    "check-types": "tsc",
    "eject": "react-scripts eject",
    "format": "yarn run prettier --write && stylelint --ignore-path .gitignore --fix \"**/*.+(css|tsx)\"",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "validate": "npm-run-all --parallel check-types check-format lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run check-types && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|ts|tsx)": [
      "prettier --write",
      "git add"
    ],
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-prettier/recommended",
      "stylelint-config-rational-order"
    ],
    "plugins": [
      "stylelint-order"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "^0.4.1",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@reduxjs/toolkit": "^1.0.4",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/webpack-env": "^1.14.1",
    "axios": "^0.19.0",
    "emotion-theming": "^10.0.19",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.2.0",
    "react-simple-maps": "^1.0.0-beta.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/d3-fetch": "^1.1.5",
    "@types/d3-scale": "^2.1.1",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "sort-package-json": "^1.31.0",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-selector-bem-pattern": "^2.1.0"
  }
}
